package utils

import "time"

func GetTimestampFromString(tm []byte) (t time.Time) {
	// year
	len := 0
	target := string(tm[:len+4]) + "-"
	len += 4
	// month
	target += string(tm[len:len+2]) + "-"
	len += 2
	// day
	target += string(tm[len:len+2]) + "T"
	len += 2
	//hour
	target += string(tm[len:len+2]) + ":"
	len += 2
	//minute
	target += string(tm[len:len+2]) + ":"
	len += 2
	//sec
	target += string(tm[len:len+2]) + "+00:00"
	len += 2

	t, _ = time.Parse(
		time.RFC3339,
		target)

	return t
}
